# Public or server-side (either pair works)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
# Server-side fallbacks (optional if you use the public pair)
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Service role (not used at runtime in clients; keep safe and server-only)
SUPABASE_SERVICE_ROLE_KEY=

# Database
DATABASE_URL=

# App
NEXT_PUBLIC_SITE_URL=
# Optional private fallback if you can't expose NEXT_PUBLIC_SITE_URL
SITE_URL=
# Optional override for OAuth/email callback host (use when previews should redirect to production).
# Provide the base domain only (no path), e.g., https://proofound.io
NEXT_PUBLIC_AUTH_REDIRECT_URL=
AUTH_REDIRECT_URL=
NODE_ENV=development
